generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model userDetails {
  id Int @id @default(autoincrement())
  email String @unique
  name String?
  username String @unique
  password String 
}

model jobDetail {
  id Int @id @default(autoincrement())
  jobTitle String
  jobDescription String
}