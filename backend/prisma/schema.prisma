generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-3.0.x"]
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model userAuth {
  id Int @id @default(autoincrement())
  email String @unique
  name String?
  username String @unique
  password String 
}

model jobDetail {
  id Int @id @default(autoincrement())
  jobTitle String
  jobDescription String
}